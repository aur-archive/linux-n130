# mkinitcpio preset file for the 'linux-N130' package
ALL_config="/etc/mkinitcpio-N130.conf"
ALL_kver="/boot/vmlinuz-N130"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-N130.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-N130-fallback.img" 
fallback_options="-S autodetect"
